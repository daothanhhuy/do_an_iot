<div class="container-fluid mt-0 p-0">
    <h2 class="mb-4">{{tittle}}</h2>
    <div class="row">
        <div class="col col-4">
            <img src="data:image/{{items.img.contentType}};base64,{{{toString64 items.img.data}}}" alt="Who enter the room" width="100%" height="400">
        </div>
        <div class="col col-6">
            <h3>Devicce name: {{items.name}}</h3>
            <h3>Owned Device name: {{items.attachTo}}</h3>
            <h3>Device IP : {{items.ip}}</h3>
            <h3>Took at: {{items.createdAt}}</h3>
        </div>
    </div>
</div>